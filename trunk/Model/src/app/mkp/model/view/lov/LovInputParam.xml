<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="LovInputParam"
  Version="11.1.1.64.93"
  InheritPersonalization="true"
  FromList="INPUT_PARAM InputParam"
  BindingStyle="OracleName"
  CustomQuery="false"
  PageIterMode="Full"
  UseGlueCode="false"
  SelectList="InputParam.PARAM_ID, 
       InputParam.PARAM_NM, 
       InputParam.DESCR"
  OrderBy="InputParam.DESCR">
  <DesignTime>
    <Attr Name="_codeGenFlag2" Value="Access|VarAccess"/>
  </DesignTime>
  <EntityUsage
    Name="InputParam"
    Entity="app.mkp.model.entity.param.InputParam"/>
  <ViewAttribute
    Name="ParamId"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="ParamId"
    EntityUsage="InputParam"
    AliasName="PARAM_ID"/>
  <ViewAttribute
    Name="ParamNm"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="ParamNm"
    EntityUsage="InputParam"
    AliasName="PARAM_NM"/>
  <ViewAttribute
    Name="Label"
    IsUpdateable="false"
    IsSelected="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="VIEW_ATTR"
    SQLType="VARCHAR">
    <RecalcCondition><![CDATA[true]]></RecalcCondition>
    <TransientExpression><![CDATA[return ParamNm + " [" + ParamId + "]";]]></TransientExpression>
    <Dependencies>
      <Item
        Value="ParamId"/>
      <Item
        Value="ParamNm"/>
    </Dependencies>
  </ViewAttribute>
  <ViewAttribute
    Name="Descr"
    PrecisionRule="true"
    EntityAttrName="Descr"
    EntityUsage="InputParam"
    AliasName="DESCR"/>
</ViewObject>
