<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="PersonView"
  Version="11.1.1.64.93"
  InheritPersonalization="true"
  SelectList="Person.EMPLID, 
       Person.USERNAME, 
       Person.FIRSTNAME, 
       Person.MIDDLENAME, 
       Person.LASTNAME, 
       Person.DISPLAYNAME, 
       Person.TITLE, 
       Person.EMAIL, 
       Person.PHONE, 
       Person.UUIDPERSON"
  FromList="PERSON Person"
  BindingStyle="OracleName"
  CustomQuery="false"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_codeGenFlag2" Value="Access|VarAccess"/>
  </DesignTime>
  <EntityUsage
    Name="Person"
    Entity="app.mkp.model.entity.exchange.Person"/>
  <ViewAttribute
    Name="Emplid"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="Emplid"
    EntityUsage="Person"
    AliasName="EMPLID"/>
  <ViewAttribute
    Name="Username"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="Username"
    EntityUsage="Person"
    AliasName="USERNAME"/>
  <ViewAttribute
    Name="Firstname"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="Firstname"
    EntityUsage="Person"
    AliasName="FIRSTNAME"/>
  <ViewAttribute
    Name="Middlename"
    PrecisionRule="true"
    EntityAttrName="Middlename"
    EntityUsage="Person"
    AliasName="MIDDLENAME"/>
  <ViewAttribute
    Name="Lastname"
    PrecisionRule="true"
    EntityAttrName="Lastname"
    EntityUsage="Person"
    AliasName="LASTNAME"/>
  <ViewAttribute
    Name="Displayname"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="Displayname"
    EntityUsage="Person"
    AliasName="DISPLAYNAME"/>
  <ViewAttribute
    Name="Title"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="Title"
    EntityUsage="Person"
    AliasName="TITLE"/>
  <ViewAttribute
    Name="Email"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="Email"
    EntityUsage="Person"
    AliasName="EMAIL"/>
  <ViewAttribute
    Name="Phone"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="Phone"
    EntityUsage="Person"
    AliasName="PHONE"/>
  <ViewAttribute
    Name="Uuidperson"
    PrecisionRule="true"
    EntityAttrName="Uuidperson"
    EntityUsage="Person"
    AliasName="UUIDPERSON"/>
  <ViewAttribute
    Name="Fullname"
    IsUpdateable="false"
    IsSelected="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="VIEW_ATTR"
    SQLType="VARCHAR">
    <RecalcCondition><![CDATA[true]]></RecalcCondition>
    <TransientExpression><![CDATA[if(Middlename == null && Lastname == null){return Firstname} else if(Middlename == null && Lastname != null){return Firstname+' '+Lastname} else if(Middlename != null && Lastname == null){return Firstname+' '+Middlename}else if(Middlename != null && Lastname != null){return Firstname+' '+Middlename+' '+Lastname}]]></TransientExpression>
    <Dependencies>
      <Item
        Value="Firstname"/>
      <Item
        Value="Lastname"/>
      <Item
        Value="Middlename"/>
    </Dependencies>
  </ViewAttribute>
  <ViewLinkAccessor
    Name="PositionView"
    ViewLink="app.mkp.model.view.exchange.PositionPersonFkLink"
    Type="oracle.jbo.RowIterator"
    IsUpdateable="false"/>
</ViewObject>
