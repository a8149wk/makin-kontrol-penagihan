  <SQLQuery>
    <![CDATA[SELECT UZ.UNITCOMPANY_ID, UZ.DEPARTMENT_ID, UZ.SUPPLIER_ID, UZ.KONTRAK_NO, UZ.PO_NO, UZ.CURRENCY_ID, UZ.NIL_MAX_TRAN, UZ.UTIL, (UZ.UTIL / UZ.NIL_MAX_TRAN) * 100 AS UTIL_PERCENTAGE
FROM (
SELECT MT.UNITCOMPANY_ID, MT.DEPARTMENT_ID, MT.SUPPLIER_ID, MT.KONTRAK_NO, MT.PO_NO, MT.CURRENCY_ID, 
(SELECT MK.NILAI_KONTRAK FROM MKN_KONTRAK MK WHERE MK.JNS_MATA_UANG = MT.CURRENCY_ID AND MK.NO_KONTRAK = MT.KONTRAK_NO 
UNION ALL
SELECT MK.NILAI_KONTRAK_2 FROM MKN_KONTRAK MK WHERE MK.JNS_MATA_UANG_2 = MT.CURRENCY_ID AND MK.NO_KONTRAK=MT.KONTRAK_NO
UNION ALL
SELECT MK.NILAI_KONTRAK_3 FROM MKN_KONTRAK MK WHERE MK.JNS_MATA_UANG_3 = MT.CURRENCY_ID AND MK.NO_KONTRAK=MT.KONTRAK_NO
UNION ALL
SELECT MK.NILAI_KONTRAK_4 FROM MKN_KONTRAK MK WHERE MK.JNS_MATA_UANG_4 = MT.CURRENCY_ID AND MK.NO_KONTRAK=MT.KONTRAK_NO
) AS NIL_MAX_TRAN,
(SELECT SUM(MT2.AMOUNT) FROM MKN_TTF MT2 WHERE MT2.KONTRAK_NO = MT.KONTRAK_NO AND MT2.CURRENCY_ID = MT.CURRENCY_ID) AS UTIL
FROM MKN_TTF MT WHERE MT.KONTRAK_NO IS NOT NULL
GROUP BY MT.UNITCOMPANY_ID, MT.DEPARTMENT_ID, MT.SUPPLIER_ID, MT.KONTRAK_NO, MT.PO_NO, MT.CURRENCY_ID
) UZ]]>
