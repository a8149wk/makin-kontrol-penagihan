<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="ListPtView"
  Version="11.1.1.64.93"
  InheritPersonalization="true"
  SelectList="MasterPt.KD_MASTER, 
       MasterPt.JNS_MASTER, 
       MasterPt.DESCR"
  FromList="MASTER_PT MasterPt"
  BindingStyle="OracleName"
  CustomQuery="false"
  PageIterMode="Full"
  UseGlueCode="false"
  Where="MasterPt.JNS_MASTER = 'APR'"
  OrderBy="MasterPt.DESCR">
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="app.mkp.model.view.param.ListPtView_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <EntityUsage
    Name="MasterPt"
    Entity="app.mkp.model.entity.param.MasterPt"
    ReadOnly="true"
    DeleteParticipant="false"/>
  <ViewAttribute
    Name="KdMaster"
    IsUpdateable="false"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="KdMaster"
    EntityUsage="MasterPt"
    AliasName="KD_MASTER"/>
  <ViewAttribute
    Name="JnsMaster"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="JnsMaster"
    EntityUsage="MasterPt"
    AliasName="JNS_MASTER"/>
  <ViewAttribute
    Name="Descr"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="Descr"
    EntityUsage="MasterPt"
    AliasName="DESCR"/>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="app.mkp.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
